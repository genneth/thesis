@Article{Wu2010,
  Title                    = {{Thermodynamics and Kinetics of the Complexation Reaction of Lead by Calix-DANS4.}},
  Author                   = {Wu, Ting and Leray, Isabelle and G\'{e}not, Val\'{e}rie and Lef\`{e}vre, Jean Pierre and Korovitch, Alexandre and Ha-Duong, Nguy\^{e}t-Thanh and {El Hage Chahine}, Jean-Michel and Delaire, Jacques Alexis},
