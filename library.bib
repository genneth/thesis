journal = {Developmental dynamics : an official publication of the American Association of Anatomists},
keywords = {Animals,Bromodeoxyuridine,Bromodeoxyuridine: metabolism,Cell Count,Cell Cycle,Cell Death,Cell Division,Fluorescence,Microscopy,Mitosis,Retina,Retina: cytology,Retina: embryology,Retina: metabolism,Time Factors,Zebrafish,Zebrafish: embryology,Zebrafish: metabolism},
month = nov,
number = {3},
pages = {391--401},
pmid = {11066095},
title = {{Modulation of cell proliferation in the embryonic retina of zebrafish (Danio rerio).}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11066095},
volume = {219},
year = {2000}
