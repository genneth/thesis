pages = {62--9},
pmid = {24355609},
title = {{Biased competition between Lgr5 intestinal stem cells driven by oncogenic mutation induces clonal expansion.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24355609},
volume = {15},
year = {2014}
}
