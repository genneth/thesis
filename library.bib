@article{Cox1989,
abstract = {Let $\eta$t be the basic voter model on Zd and let $\eta$(N) t be the voter model on $\Lambda$(N), the torus of side N in Zd. Unlike $\eta$t, $\eta$(N) t (for fixed N) gets trapped with probability 1 as t →∞ at all 0's or all 1's. We examine the asymptotic growth of these trapping or consensus times $\tau$(N) as N →∞. To do this we obtain limit theorems for coalescing random walk systems on the torus $\Lambda$(N), including a new hitting time limit theorem for (noncoalescing) random walk on the torus.},
author = {Cox, J. Theodore},
journal = {The Annals of Probability},
number = {4},
pages = {1333--1366},
title = {{Coalescing Random Walks and Voter Model Consensus Times on the Torus in \$Z\^{}d\$}},
url = {http://www.jstor.org/stable/2244439},
volume = {7},
year = {1989}
}
